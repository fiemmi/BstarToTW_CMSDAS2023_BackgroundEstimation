{
    "NAME": "ttbar",
    "GLOBAL": {
	"FILE": "all_plots.root",
    "FILE_SIG": "TTbarAllHad$process.root",
	"FILE_UP": "TTbarAllHad$process_$syst_up.root",
	"FILE_DOWN": "TTbarAllHad$process_$syst_down.root",
    "HIST": "$process_$region_prefit_2D",
    "HIST_SIG": "MttvsMt$region", 
	"HIST_UP": "MttvsMt$region$systup",
	"HIST_DOWN": "MttvsMt$region$systdown",
	"path": "/afs/cern.ch/user/m/mmorris/BackgroundEstimation/CMSSW_10_6_14/src/BstarToTW_CMSDAS2023_BackgroundEstimation/regions/2016/medium/ttbarfits_cen1b_3x1/ttbar-RSGluon2000_area/plots_fit_b",
	"path_sig": "/afs/cern.ch/user/m/mmorris/public/ttbarAllHad/twodalphabet/Run2_deepak8_medium_categories",
	"SIGNAME": ["signalRSGluon2000"]
    },
    "REGIONS": {
        "cen1bPass": {
            "PROCESSES": ["16_SIGNAME","16_TTbar"],
            "BINNING": "default"
            },
        "cen1bFail": {
                "PROCESSES": ["16_SIGNAME","16_TTbar"],
                "BINNING": "default"
            }
    },
    "PROCESSES": {
        "data_obs": {
                "SYSTEMATICS": [],
                "SCALE": 1.0,
                "COLOR": 1,
                "TYPE": "DATA",
                "TITLE": "Data",
                "HELP": "since the data file is not called TTbarAllHaddata_obs.root, we make an alias to the real name - shown below",
                "LOC": "path/FILE:HIST"
            },
        "16_SIGNAME": {
                "SYSTEMATICS":["lumi", "btag_unc"],
                "SCALE": 1.0, 
                "COLOR": 1, 
                "TYPE": "SIGNAL",
                "TITLE": "g_{RS} (3 TeV)",
                "LOC": "path_sig/FILE_SIG:HIST_SIG"
            },
            "16_TTbar": {
                "SYSTEMATICS": ["lumi", "ttbar_xsec", "btag_unc"],
                "SCALE": 1.0,
                "COLOR": 2,
                "TYPE": "BKG",
                "TITLE": "t#bar{t}",
                "LOC": "path/FILE:HIST"
            }
        
        },
     "SYSTEMATICS": {
        "lumi": {
            "CODE": 0,
            "VAL": 1.018
        },
        "ttbar_xsec": {
            "CODE": 0,
            "VAL": 1.20
        },
        "btag_unc": {
            "CODE": 0,
            "VAL": 1.0
        },
        "pileup": {
	    "ALIAS": "PILEUP",
            "CODE": 2,
	    "UP": "path/FILE:HIST_UP",
	    "DOWN": "path/FILE:HIST_DOWN",
            "SCALE": 1.0,
	    "SIGMA": 1.0
        },
        "pdf": {
	    "ALIAS": "PDF",
            "CODE": 2,
	    "UP": "path/FILE:HIST_UP",
	    "DOWN": "path/FILE:HIST_DOWN",
            "SCALE": 1.0,
	    "SIGMA": 1.0
        },
        "jes": {
	    "ALIAS": "JES",
            "CODE": 2,
	    "UP": "path/FILE:HIST_UP",
	    "DOWN": "path/FILE:HIST_DOWN",
            "SCALE": 1.0,
	    "SIGMA": 1.0
        },
        "jer": {
	    "ALIAS": "JER",
            "CODE": 2,
	    "UP": "path/FILE:HIST_UP",
	    "DOWN": "path/FILE:HIST_DOWN",
            "SCALE": 1.0,
	    "SIGMA": 1.0
        },
        "btag": {
	    "ALIAS": "BTAG",
            "CODE": 2,
	    "UP": "path/FILE:HIST_UP",
	    "DOWN": "path/FILE:HIST_DOWN",
            "SCALE": 1.0,
	    "SIGMA": 1.0
        }
     },
    
    "BINNING": {
	"default": {
	    "X": {
		"NAME": "jetmass",
		"TITLE": "m_{t} [GeV]",
		"BINS":[25, 50, 75, 100, 125, 150, 175, 200, 225, 250, 275, 300, 325, 350, 375, 400, 425, 450, 475],
		"SIGSTART": 125,
		"SIGEND": 225
	    },
	    "Y": {
		"NAME": "resmass",
		"TITLE": "m_{t#bar{t}} [GeV]",
		"BINS": [800, 1100, 1400, 1700, 2000, 2300, 2600, 2900, 3200, 3500, 3800, 4100, 4400, 4700, 5000]
	    }
	}
    },
    "OPTIONS": {
	"blindedPlots": [],
	"blindedFit": [],
        "plotPrefitSigInFitB": true,
	"plotUncerts": false,
	"plotTitles": false,
