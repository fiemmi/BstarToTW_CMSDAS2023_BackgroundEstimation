{
    "NAME": "ttbar",
    "GLOBAL": {
	"FILE": "TTbarAllHad$process.root",
	"FILE_UP": "TTbarAllHad$process_$syst_up.root",
	"FILE_DOWN": "TTbarAllHad$process_$syst_down.root",
	"HIST": "MttvsMt$region",
	"HIST_UP": "MttvsMt$region$systup",
	"HIST_DOWN": "MttvsMt$region$systdown",
	"path": "/afs/cern.ch/user/m/mmorris/public/ttbarAllHad/twodalphabet/Run2_deepak8_tight_categories",
	"SIGNAME": [
                "signalRSGluon1000",
                "signalRSGluon1500",
                "signalRSGluon2000",
                "signalRSGluon2500",
                "signalRSGluon3000",
                "signalRSGluon3500",
                "signalRSGluon4000",
                "signalRSGluon4500",
                "signalRSGluon5000",
                
                "signalZPrime1000_DM",
                "signalZPrime1500_DM",
                "signalZPrime2000_DM",
                "signalZPrime2500_DM",
                "signalZPrime3000_DM",
                "signalZPrime3500_DM",
                "signalZPrime4000_DM",
                "signalZPrime4500_DM",
                "signalZPrime5000_DM",
                
                "signalZPrime1000_10",
                "signalZPrime2000_10",
                "signalZPrime3000_10",
                "signalZPrime4000_10",
                
                "signalZPrime1000_30",
                "signalZPrime2000_30",
                "signalZPrime3000_30",
                "signalZPrime4000_30"
               ]
    },
    "REGIONS": {
        "cen0bPass": {
            "PROCESSES": ["16_SIGNAME","16_TTbar"],
            "BINNING": "default"
            },
        "cen0bFail": {
                "PROCESSES": ["16_SIGNAME","16_TTbar"],
                "BINNING": "default"
            }
    },
    "PROCESSES": {
        "data_obs": {
                "SYSTEMATICS": [],
                "SCALE": 1.0,
                "COLOR": 1,
                "TYPE": "DATA",
                "TITLE": "Data",
                "HELP": "since the data file is not called TTbarAllHaddata_obs.root, we make an alias to the real name - shown below",
                "ALIAS": "16_Data",
                "LOC": "path/FILE:HIST"
            },
        "16_SIGNAME": {
                "SYSTEMATICS":["lumi", "pileup", "pdf", "jes", "jer"],
                "SCALE": 1.0, 
                "COLOR": 1, 
                "TYPE": "SIGNAL",
                "TITLE": "g_{RS} (3 TeV)",
                "LOC": "path/FILE:HIST"
            },
            "16_TTbar": {
                "SYSTEMATICS": ["lumi","ttbar_xsec", "pileup", "pdf", "jes", "jer"],
                "SCALE": 1.0,
                "COLOR": 2,
                "TYPE": "BKG",
                "TITLE": "t#bar{t}",
                "LOC": "path/FILE:HIST"
            }
        
        },
     "SYSTEMATICS": {
        "lumi": {
            "CODE": 0,
            "VAL": 1.018
        },
        "ttbar_xsec": {
            "CODE": 0,
            "VAL": 1.20
        },
        "pileup": {
	    "ALIAS": "PILEUP",
            "CODE": 2,
	    "UP": "path/FILE:HIST_UP",
	    "DOWN": "path/FILE:HIST_DOWN",
            "SCALE": 1.0,
	    "SIGMA": 1.0
        },
        "pdf": {
	    "ALIAS": "PDF",
            "CODE": 2,
	    "UP": "path/FILE:HIST_UP",
	    "DOWN": "path/FILE:HIST_DOWN",
            "SCALE": 1.0,
	    "SIGMA": 1.0
        },
        "jes": {
	    "ALIAS": "JES",
            "CODE": 2,
	    "UP": "path/FILE:HIST_UP",
	    "DOWN": "path/FILE:HIST_DOWN",
            "SCALE": 1.0,
	    "SIGMA": 1.0
        },
        "jer": {
	    "ALIAS": "JER",
            "CODE": 2,
	    "UP": "path/FILE:HIST_UP",
	    "DOWN": "path/FILE:HIST_DOWN",
            "SCALE": 1.0,
	    "SIGMA": 1.0
        }
     },
    
    "BINNING": {
	"default": {
	    "X": {
		"NAME": "jetmass",
		"TITLE": "m_{t} [GeV]",
		"BINS":[25, 50, 75, 100, 125, 150, 175, 200, 225, 250, 275, 300, 325, 350, 375, 400, 425, 450, 475],
		"SIGSTART": 125,
		"SIGEND": 225
	    },
	    "Y": {
		"NAME": "resmass",
		"TITLE": "m_{t#bar{t}} [GeV]",
		"BINS": [800, 1160, 1520, 1880, 2240, 2600, 2960, 3320, 3680]
	    }
	}
    },
    "OPTIONS": {
	"blindedPlots": [],
	"blindedFit": [],
        "plotPrefitSigInFitB": true,
	"plotUncerts": false,
	"plotTitles": false,
	"plotTemplateComparisons": false,
	"plotEvtsPerUnit": true,
	"year": 2
    }
}
