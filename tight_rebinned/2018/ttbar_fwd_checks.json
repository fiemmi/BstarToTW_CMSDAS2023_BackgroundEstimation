{
    "NAME": "ttbar",
    "GLOBAL": {
	"FILE": "TTbarAllHad$process.root",
	"FILE_UP": "TTbarAllHad$process_$syst_up.root",
	"FILE_DOWN": "TTbarAllHad$process_$syst_down.root",
	"HIST": "MttvsMt$region",
	"HIST_UP": "MttvsMt$region$systup",
	"HIST_DOWN": "MttvsMt$region$systdown",
	"path": "/eos/home-m/mmorris/Documents/TTbarResonance/twodalphabet/Run2_deepak8_tight_to_mediumnotight_rebinned/",
	"SIGNAME": [
                "signalRSGluon1000",
                "signalRSGluon1500",
                "signalRSGluon2000",
                "signalRSGluon2500",
                "signalRSGluon3000",
                "signalRSGluon3500",
                "signalRSGluon4000",
                "signalRSGluon4500",
                "signalRSGluon5000",
                "signalRSGluon5500",
                "signalRSGluon6000"
        ]
    },
    "REGIONS": {
        "fwd18Pass": {
            "PROCESSES": ["18_SIGNAME","18_TTbar"],
            "BINNING": "default"
            },
        "fwd18Fail": {
                "PROCESSES": ["18_SIGNAME","18_TTbar"],
                "BINNING": "default"
            }
    },
    "PROCESSES": {
        "data_obs": {
                "SYSTEMATICS": [],
                "SCALE": 1.0,
                "COLOR": 1,
                "TYPE": "DATA",
                "TITLE": "Data",
                "HELP": "since the data file is not called TTbarAllHaddata_obs.root, we make an alias to the real name - shown below",
                "ALIAS": "18_Data",
                "LOC": "path/FILE:HIST"
            },
        "18_SIGNAME": {
                "SYSTEMATICS":["lumi", "btag_unc", "pileup", "pdf", "jes", "jer", "btag", "q2"],
                "SCALE": 1.0, 
                "COLOR": 1, 
                "TYPE": "SIGNAL",
                "TITLE": "g_{RS} (2 TeV)",
                "LOC": "path/FILE:HIST"
            },
            "18_TTbar": {
                "SYSTEMATICS": ["lumi", "ttbar_xsec", "btag_unc", "pileup", "pdf", "jes", "jer", "btag", "q2"],
                "SCALE": 1.0,
                "COLOR": 2,
                "TYPE": "BKG",
                "TITLE": "t#bar{t}",
                "LOC": "path/FILE:HIST"
            }
        
        },
     "SYSTEMATICS": {
        "lumi": {
            "CODE": 0,
            "VAL": 1.018
        },
        "ttbar_xsec": {
            "CODE": 0,
            "VAL": 1.20
        },
        "btag_unc": {
            "CODE": 0,
            "VAL": 1.0
        },
        "pileup": {
	    "ALIAS": "PILEUP",
            "CODE": 2,
	    "UP": "path/FILE:HIST_UP",
	    "DOWN": "path/FILE:HIST_DOWN",
            "SCALE": 1.0,
	    "SIGMA": 1.0
        },
        "pdf": {
	    "ALIAS": "PDF",
            "CODE": 2,
	    "UP": "path/FILE:HIST_UP",
	    "DOWN": "path/FILE:HIST_DOWN",
            "SCALE": 1.0,
	    "SIGMA": 1.0
        },
        "jes": {
	    "ALIAS": "JES",
            "CODE": 2,
	    "UP": "path/FILE:HIST_UP",
	    "DOWN": "path/FILE:HIST_DOWN",
            "SCALE": 1.0,
	    "SIGMA": 1.0
        },
        "jer": {
	    "ALIAS": "JER",
            "CODE": 2,
	    "UP": "path/FILE:HIST_UP",
	    "DOWN": "path/FILE:HIST_DOWN",
            "SCALE": 1.0,
	    "SIGMA": 1.0
        },
        "btag": {
	    "ALIAS": "BTAG",
            "CODE": 2,
	    "UP": "path/FILE:HIST_UP",
	    "DOWN": "path/FILE:HIST_DOWN",
            "SCALE": 1.0,
	    "SIGMA": 1.0
        },
        "q2": {
	    "ALIAS": "Q2",
            "CODE": 2,
	    "UP": "path/FILE:HIST_UP",
	    "DOWN": "path/FILE:HIST_DOWN",
            "SCALE": 1.0,
	    "SIGMA": 1.0
        },
        "prefiring": {
	    "ALIAS": "PREFIRING",
            "CODE": 2,
	    "UP": "path/FILE:HIST_UP",
	    "DOWN": "path/FILE:HIST_DOWN",
            "SCALE": 1.0,
	    "SIGMA": 1.0
        }
     },
    
    "BINNING": {
	"default": {
	    "X": {
		"NAME": "jetmass",
		"TITLE": "m_{t} [GeV]",
		"BINS":[25,50,75,105,125,145,165,195,210,225,250,275,300, 325, 350,375, 475],
		"SIGSTART": 125,
		"SIGEND": 225
	    },
	    "Y": {
		"NAME": "resmass",
		"TITLE": "m_{t#bar{t}} [GeV]",
		"BINS": [800, 1000, 1300, 1600, 1900, 2200, 2500, 2800, 3100, 3400, 3700, 6500]
	    }
	}
    },
    "OPTIONS": {
	"blindedPlots": [],
	"blindedFit": [],
        "plotPrefitSigInFitB": true,
	"plotUncerts": false,
	"plotTitles": false,
	"plotTemplateComparisons": false,
	"plotEvtsPerUnit": true,
	"year": 18
    }
}
