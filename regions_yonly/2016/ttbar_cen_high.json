            },
            "16_TTbar": {
                "SYSTEMATICS": ["lumi","ttbar_xsec", "btag_unc", "pileup", "pdf", "jes", "jer", "btag", "q2", "prefiring"],
                "SCALE": 1.0,
                "COLOR": 2,
                "TYPE": "BKG",
                "TITLE": "t#bar{t}",
                "LOC": "path/FILE:HIST"
            }
        
        },
