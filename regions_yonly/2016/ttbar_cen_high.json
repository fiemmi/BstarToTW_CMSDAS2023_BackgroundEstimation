                "LOC": "path/FILE:HIST"
            },
        "16_SIGNAME": {
                "SYSTEMATICS":["lumi", "btag_unc", "pileup", "pdf", "jes", "jer", "btag", "q2", "prefiring"],
                "SCALE": 1.0, 
                "COLOR": 1, 
                "TYPE": "SIGNAL",
